<?xml version="1.0" encoding="UTF-8"?>

<suite name="Test Suite Room Manager Application" parallel="false">
	<listeners>
	      <listener class-name="org.uncommons.reportng.HTMLReporter"/>
	      <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
	      <listener class-name="org.roommanager.common.ScreenshotManager"/>
	</listeners>	
   <test name="Test Add Resource">
    <classes>
      <class name="org.roommanager.test.admin.resource.AddResource" />
    </classes>
  </test>
  <test name="Test Remove Resource">
    <classes>
      <class name="org.roommanager.test.admin.resource.RemoveResource" />
    </classes>
  </test>
</suite>